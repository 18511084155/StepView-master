<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StepView">
        <attr name="sv_selectedCircleColor" format="color" />
        <attr name="sv_unSelectedCircleColor" format="color" />
        <attr name="sv_unSelectedCircleRadius" format="dimension" />
        <attr name="sv_selectedCircleRadius" format="dimension" />
        <attr name="sv_selectedTextColor" format="color" />
        <attr name="sv_selectedStepNumberColor" format="color" />
        <attr name="sv_isShowStepNumber" format="boolean" />
        <attr name="sv_stepNumberTextSize" format="dimension" />
        <attr name="sv_doneStepMarkColor" format="color" />
        <attr name="sv_doneCircleColor" format="color" />
        <attr name="sv_doneCircleRadius" format="dimension" />
        <attr name="sv_doneTextColor" format="color" />
        <attr name="sv_nextTextColor" format="color" />
        <attr name="sv_nextStepLineColor" format="color" />
        <attr name="sv_doneStepLineColor" format="color" />
        <attr name="sv_stepPadding" format="dimension" />
        <attr name="sv_textPadding" format="dimension" />
        <attr name="sv_stepLineWidth" format="dimension" />

        <attr name="sv_bottomTextSize" format="dimension|reference"/>
        <attr name="sv_viewBackground" format="reference"/>
        <attr name="sv_viewAnimationDuration" format="integer" />

        <attr name="sv_stepViewStyle" format="reference" />
        <attr name="sv_viewAnimationType" format="enum">
            <enum name="Line" value="0" />
            <enum name="Circle" value="1" />
            <enum name="All" value="2" />
            <enum name="None" value="3" />
        </attr>
        <attr name="sv_stepsNumber" format="integer" />
        <attr name="sv_steps" format="reference" />
        <attr name="sv_typeface" format="reference" />
        <attr name="sv_isReceive" format="boolean" />
        <attr name="sv_undoDrawable" format="reference" />
        <attr name="sv_iconDrawableWith" format="dimension|reference"/>
        <attr name="sv_iconDrawableHeight" format="dimension|reference"/>
        <attr name="sv_underwayDrawable" format="reference" />
        <attr name="sv_completedDrawable" format="reference" />
        <attr name="sv_giftDrawable" format="reference" />
        <attr name="sv_openGiftDrawable" format="reference" />
        <attr name="sv_giftDrawableWith" format="dimension|reference"/>
        <attr name="sv_giftDrawableHeight" format="dimension|reference"/>
        <attr name="sv_iconDrawablePadding" format="dimension|reference"/>
    </declare-styleable>
</resources>